                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Username" />
                  <Label text="Password" GridPane.rowIndex="1" />
                  <ButtonBar GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <buttons>
                      <Button mnemonicParsing="false" onAction="#handleLogin" text="Log in" />
                     </buttons>
                  </ButtonBar>
                  <TextField fx:id="username" GridPane.columnIndex="1" />
                  <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
      <BorderPane prefHeight="134.0" prefWidth="290.0" GridPane.columnIndex="1">
         <center>
            <Label text="Jar Jar Binks">
